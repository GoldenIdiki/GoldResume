            <div class="resume-section-content">
                <h2 class="mb-5">Skills</h2>
                <div class="subheading mb-3">Programming Languages & Tools</div>
                <ul class="list-inline dev-icons" style="font-size:30px">
                    <li class="list-inline-item">C#,</li>
                    <li class="list-inline-item">ASP.NET Core MVC,</li>
                    <li class="list-inline-item">ASP.NET Core API,</li>
                    <li class="list-inline-item">Entity Framework,</li>
                    <li class="list-inline-item">MSSQL,</li>
                    <li class="list-inline-item">SQLite,</li>
                    <li class="list-inline-item">MySQL,</li>
                    <li class="list-inline-item">Python,</li>
                    <li class="list-inline-item">JavaScript,</li>
                    <li class="list-inline-item">PHP,</li>
                    <li class="list-inline-item">Jira</li>
                </ul>
                <div class="subheading mb-3">Workflow</div>
                <ul class="fa-ul mb-0">
                    <li>
                        <span class="fa-li"><i class="fas fa-check"></i></span>
                        Mobile-First, Responsive Design
                    </li>
                    <li>
                        <span class="fa-li"><i class="fas fa-check"></i></span>
                        Cross Browser Testing & Debugging
                    </li>
                    <li>
                        <span class="fa-li"><i class="fas fa-check"></i></span>
                        Cross Functional Teams
                    </li>
                    <li>
                        <span class="fa-li"><i class="fas fa-check"></i></span>
                        Agile Development & Scrum
                    </li>
                </ul>
            </div>
        </section>
        <hr class="m-0" />
        <!-- Interests-->
        <section class="resume-section" id="interests">
            <div class="resume-section-content">
                <h2 class="mb-5">Interests</h2>
                <p>I enjoy playing and watching soccer</p>
                <p class="mb-0">I enjoy playing chess</p>
                <p class="mb-0">When stressed-out, I listen to cool music and maybe sleep off</p>
            </div>
        </section>
        <hr class="m-0" />
        <!-- Awards-->
        <section class="resume-section" id="contactme">
            <div class="resume-section-content">
                <h2 class="mb-5">Contact Me</h2>
                <div class="row justify-content-lg-start">
                    <div class="col-lg-8 col-xl-7">
                        <!-- * * * * * * * * * * * * * * *-->
                        <!-- * * SB Forms Contact Form * *-->
                        <!-- * * * * * * * * * * * * * * *-->
                        <!-- This form is pre-integrated with SB Forms.-->
                        <!-- To make this form functional, sign up at-->
                        <!-- https://startbootstrap.com/solution/contact-forms-->
                        <!-- to get an API token!-->
                        <form method="post" asp-action="Index">
                            <div class="text-danger">@ViewBag.Message</div>
                            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                            <!-- Name input-->
                            <div class="form-floating mb-3">
                                <div>
                                    <label asp-for="FullName"></label>
                                </div>
                                <input class="form-control" asp-for="FullName" type="text" placeholder="Enter your name..." />
                                <span asp-validation-for="FullName" class="text-danger"></span>
                                @*<div class="invalid-feedback" data-sb-feedback="name:required">A name is required.</div>*@
                            </div>
                            <!-- Email address input-->
                            <div class="form-floating mb-3">
                                <div>
                                    <label asp-for="Email"></label>
                                </div>
                                <input class="form-control" asp-for="Email" type="email" placeholder="name@example.com" />
                                <span asp-validation-for="Email" class="text-danger"></span>
                                @*<div class="invalid-feedback" data-sb-feedback="email:required">An email is required.</div>
            <*@<!--div class="invalid-feedback" data-sb-feedback="email:email">Email is not valid.</div>-->
                            </div>
                            <!-- Phone number input-->
                            <div class="form-floating mb-3">
                                <div>
                                    <label asp-for="PhoneNumber"></label>
                                </div>
                                <input class="form-control" asp-for="PhoneNumber" type="tel" placeholder="enter phone number" />
                                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                            </div>
                            <!-- Message input-->
                            <div class="form-floating mb-3">
                                <div>
                                    <label asp-for="Message"></label>
                                </div>
                                <textarea class="form-control" asp-for="Message" type="text" placeholder="Enter your message here..." style="height: 10rem" data-sb-validations="required"></textarea>
                                <span asp-validation-for="Message" class="text-danger"></span>
                            </div>

                            <!-- Submit Button-->
                            <button class="btn btn-primary btn-xl" type="submit">Send</button>
                            <div class="text-success">@TempData["Success"]</div>
                        </form>
                        @*server side validation*@
                        @section Scripts{
                            @{
                                                <partial name="_ValidationScriptsPartial" /> }
                        }
                    </div>
                </div>
            </div>
        </section>
    </div>
</body>
